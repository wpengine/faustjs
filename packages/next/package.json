{
  "name": "@faustjs/next",
  "version": "0.9.0",
  "description": "This module helps you use WordPress as a Headless CMS with Next.js",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build": "npm run clean && npm run ts && npm run ts:cjs && npm run package",
    "dev": "npm run ts:watch",
    "dev-lerna": "npm run dev",
    "clean": "rimraf dist",
    "package": "node ../../scripts/package.js",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --parser-options=project:tsconfig.json --fix",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --parser-options=project:tsconfig.json",
    "prepublish": "npm run build",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "ts": "tsc -p .",
    "ts:cjs": "tsc -p tsconfig-cjs.json",
    "ts:watch": "tsc -p . --watch",
    "format": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wpengine/faustjs/tree/main/packages/next"
  },
  "keywords": [
    "WordPress",
    "WP",
    "Headless",
    "CMS",
    "API",
    "WP Engine",
    "React",
    "Next",
    "Next.js"
  ],
  "author": "WP Engine",
  "license": "MIT",
  "peerDependencies": {
    "next": ">=11.1.1",
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.172",
    "@types/node": "^16.7.13",
    "@types/react": "^17.0.20",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "gqty": "^1.1.3",
    "jest": "^27.1.0",
    "next": "^11.1.2",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@gqty/logger": "^1.0.4",
    "@gqty/react": "^1.1.1",
    "@faustjs/core": "^0.9.0",
    "@faustjs/react": "^0.9.0",
    "graphql": ">=15.5",
    "lodash": "^4.17.21"
  }
}
