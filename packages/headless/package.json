{
  "name": "@wpengine/headless",
  "version": "0.6.3",
  "description": "This module helps you use WordPress as a Headless CMS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "run-s clean ts",
    "dev": "run-s clean ts:watch",
    "dev-lerna": "run-s dev",
    "clean": "rimraf dist",
    "lint:fix": "run-s clean && tsc --noEmit -p . && eslint **/*.{ts,tsx} --parser-options=project:tsconfig.json --quiet --fix",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prepublish": "run-s build",
    "generate-docs": "typedoc",
    "generate-graphql": "graphql-codegen",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "ts": "tsc -p .",
    "ts:watch": "tsc -p . --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wpengine/headless-framework/tree/main/packages/core"
  },
  "keywords": [
    "WordPress",
    "WP",
    "Headless",
    "CMS",
    "API",
    "WP Engine"
  ],
  "author": "WP Engine",
  "license": "MIT",
  "dependencies": {
    "deepmerge": "^4.2.2",
    "is-number": "^7.0.0",
    "isomorphic-fetch": "^3.0.0",
    "next-transpile-modules": "^8.0.0",
    "universal-cookie": "^4.0.4"
  },
  "peerDependencies": {
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2",
    "@apollo/client": ">=3.3",
    "graphql": ">=15.5",
    "next": ">=10.2"
  },
  "devDependencies": {
    "@apollo/client": "^3.3.4",
    "@graphql-codegen/add": "^2.0.2",
    "@graphql-codegen/cli": "^1.21.5",
    "@graphql-codegen/typescript": "^1.22.3",
    "@graphql-codegen/typescript-operations": "^1.18.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@types/express": "^4.17.12",
    "@types/is-number": "^7.0.1",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.4",
    "@types/react": "^17.0.11",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "graphql": "^15.5.1",
    "jest": "^26.6.3",
    "next": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.2.3",
    "typedoc": "^0.21.0",
    "typedoc-plugin-markdown": "^3.10.0",
    "typescript": "^4.3.4",
    "webpack": "^5.39.1",
    "webpack-cli": "^4.7.2"
  }
}
