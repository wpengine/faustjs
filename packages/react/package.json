{
  "name": "@wpengine/headless-react",
  "version": "0.6.11",
  "description": "This module helps you use WordPress as a Headless CMS with React",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "run-s clean ts",
    "dev": "run-s ts:watch",
    "dev-lerna": "run-s dev",
    "clean": "rimraf dist",
    "lint:fix": "run-s clean && tsc --noEmit -p . && eslint \"src/**/*.{ts,tsx}\" --parser-options=project:tsconfig.json --quiet --fix",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --parser-options=project:tsconfig.json",
    "prepublish": "run-s build",
    "generate-docs": "typedoc",
    "generate-graphql": "graphql-codegen",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "ts": "tsc -p .",
    "ts:watch": "tsc -p . --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wpengine/headless-framework/tree/main/packages/react"
  },
  "keywords": [
    "WordPress",
    "WP",
    "Headless",
    "CMS",
    "API",
    "WP Engine",
    "React"
  ],
  "author": "WP Engine",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^2.0.2",
    "@graphql-codegen/cli": "^1.21.4",
    "@graphql-codegen/typescript": "^1.22.0",
    "@graphql-codegen/typescript-operations": "^1.17.16",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^7.0.0",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.6",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.2.0",
    "typedoc": "^0.20.36",
    "typedoc-plugin-markdown": "^3.8.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@gqless/react": "^2.0.15",
    "@wpengine/headless-core": "^0.6.8",
    "gqless": "^2.0.14",
    "graphql": ">=15.5",
    "lodash": "^4.17.21"
  }
}
